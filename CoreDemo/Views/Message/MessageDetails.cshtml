@model EntityLayer.Concrete.Message2

@{
    ViewData["Title"] = "MessageDetails";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}

<h1>MessageDetails</h1>
<br />
@Html.Label("Message Id")
@Html.TextBoxFor(x => x.Id, new {@class = "form-control"})
<br />
@Html.Label("Message Subject")
@Html.TextBoxFor(x => x.Subject, new {@class = "form-control"})
<br />
@Html.Label("Message Centext")
@Html.TextAreaFor(x => x.MessageDetails, new {@class = "form-control"})

<button class="btn btn-info">Back</button>


@using EntityLayer.Concrete
@model List<Notification>

@{
    ViewData["Title"] = "AllNotifications";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}

<h1>Notification</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Notifications</th>
    </tr>
    @foreach(var item in Model )
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Details</td>
        </tr>
    }
</table>


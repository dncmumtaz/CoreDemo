@using EntityLayer.Concrete
@model List<Category>

@{
    Layout = null;
}

<!DOCTYPE html>

<table>
    <tr>
        <th>ID</th>
        <th>Kategory Adı</th>
        <th>Durumu</th>
        <th>Detaylar</th>
        <th>Sil</th>
    </tr>
    @foreach(var item in Model )
    {
        <tr>
            <td> @item.Id</td>
            <td> @item.Name</td>
            <td> @item.Status</td>
        </tr>
    }
</table>
